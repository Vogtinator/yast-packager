{
    include "testsuite.ycp";
    map READ = $[
        "target" : $[
            "size" : 1,
            "tmpdir" : "/tmp",
        ],
        "etc" : $[
            "install_inf": $[ 
                "InstMode":"cd",
            ]
        ],
        "probe":
            $["cdrom" : [
                $["dev_name": "/dev/hda"]
            ]]
    ];

    import "InstURL";
    import "Linuxrc";
    
    TEST (``(InstURL::installInf2Url("")), [READ], nil);
    Linuxrc::ResetInstallInf();
    READ["probe","cdrom"] = [ $["dev_name": "/dev/hdb"] , $["dev_name": "/dev/hdd"]];
    TEST (``(InstURL::RewriteCDUrl("cd:///sp2;devices=/dev/hda")), [READ], nil);
}
